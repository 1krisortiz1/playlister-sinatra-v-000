

<h1><%= @artist.name %></h1>

<h2>Song by this artist</h2>
<% @artist.songs.each do |song| %>
    <a href="/songs/<%= song.slug%>">
    <li><%= song.name %></a></li>
<% end %>

<h2>Artist genres</h2>
<% @artist.genres.each do |genre| %>
    <a href="/genres/<%= genre.slug%>">
    <li><%= genre.name %></a></li>
<% end %>
